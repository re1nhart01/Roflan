{
  "compilerOptions": {
    "experimentalDecorators": true,
    "paths": {
      "@screens/*": [
        "src/screens/*"
      ],
      "@locale": [
        "src/locale/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@type/*": [
        "src/type/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@core/*": [
        "src/core/*"
      ],
      "@reacts/*": [
        "src/reacts/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@tm/*": [
        "src/tm/*"
      ]
    },
    "lib": ["es2016","es2021", "DOM","es2018.promise"],
    "skipLibCheck": true,
  },
  "extends": "@react-native/typescript-config/tsconfig.json"

}
